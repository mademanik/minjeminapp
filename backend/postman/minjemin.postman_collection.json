							"script": {
								"exec": [
									"// Parse JSON response\r",
									"let res = pm.response.json();\r",
									"\r",
									"// Save values to environment\r",
									"pm.environment.set(\"accessToken\", res.access_token);\r",
									"pm.environment.set(\"refreshToken\", res.refresh_token);\r",
									"\r",
									"// Jika ada expiredAt (misal \"900\")\r",
									"if (res.expiredAt) {\r",
									"    pm.environment.set(\"expiredAt\", res.expires_in);\r",
									"}\r",
									"\r",
									"console.log(\"Tokens saved to sewaapp-env\");\r",
									""
